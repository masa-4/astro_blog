---
import Layout from "../layouts/Layout.astro";
const allPosts = await Astro.glob('../pages/posts/*.md');
import BlogList from '../components/BlogList.astro';
---
<Layout>
  <ul>
    <ul>
      {allPosts.map((post) => <BlogList url={post.url} title={post.frontmatter.title}/>)}
    </ul>
  </ul>
</Layout>
